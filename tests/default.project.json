{
	"name": "tina-tests",
	"globIgnorePaths": [
		"**/package.json",
		"**/tsconfig.json"
	],

	"tree": {
		"$className": "DataModel",

		"ReplicatedStorage": {
			"$className": "ReplicatedStorage",

			"include": {
				"$path": "include",

				"node_modules": {
					"$className": "Folder",
        		
					"@rbxts": {
                		"$path": "node_modules/@rbxts"
        			}
				}
			}
		},

		"ServerScriptService": {
			"$className": "ServerScriptService",

			"testBootstrap": {
				"$path": "out/testBootstrap.server.lua"
			},

			"tests": {
				"$path": "out/tests"
			}
		}
	}
}